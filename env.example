# QuestGO Environment Variables
NODE_ENV=production

# Backend Configuration
GEMINI_API_KEY=your-gemini-api-key-here
JWT_SECRET=your-super-secret-jwt-key-here
DATABASE_PATH=/app/data/questgo.json

# CORS Configuration
CORS_ORIGIN=https://questgo.ru,http://localhost:3000

# Server Configuration
BACKEND_PORT=3000
